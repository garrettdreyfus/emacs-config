using ACME
circ = @circuit begin
	Nx,Ny = 5,1
	cap = 0.05*(10^-6)
	res = 4
	induc = 0.16*(10^-3)

	j_in = voltagesource()
	prev_cap = capacitor[]
	for i in 1:Nx
		for j in 1:Nx
			if i!=1
				r1 = resistor(res)
				in1 = inductor(induc)
				r1[1]
			end
			if j!=1
				r2 = resistor(res)
				in2 = inductor(induc)
			end

			c1 = capactior(res)
			j_out = voltageprobe()
			gnd ⟷ c1[1]
			j_out[-] ⟷ gnd
			j_out[+] ⟷ c[2]
		end
	end
	j_out = voltageprobe()
	j_in[+] ⟷ r1[1]
	j_in[-] ⟷ gnd
	r1[2] ⟷ c1[1] ⟷ d1[+] ⟷ d2[-] ⟷ j_out[+]
	gnd ⟷ c1[2] ⟷ d1[-] ⟷ d2[+] ⟷ j_out[-]
end
